{
  "expo": {
    "name": "yogi-cup-mobile",
    "slug": "yogi-cup-mobile",
    "version": "1.1.1",
    "orientation": "portrait",
    "privacy": "unlisted",
    "icon": "./assets/yogi-cup-logo.png",
    "splash": {
      "image": "./assets/yogi-cup-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.yugantjoshi.yogicupmobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/yogi-cup-logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/yogi-cup-logo.png"
    }
  }
}
